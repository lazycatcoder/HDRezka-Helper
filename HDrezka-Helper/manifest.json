{
  "manifest_version": 2,
  "name": "HDrezka Helper",
  "version": "1.0",
  "description": "The extension removes ads on the HDRezka website and gets links to movies.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*.hdrezka.ac/*",
    "https://*.rezka.ag/*",
    "https://*.rezka.cc/*",
    "https://*.hdrezka.ag/*",
    "https://*.rezka-ua.tv/*",
    "https://*.rezka-ua.net/*",
    "https://*.omnirezka.tv/*",
    "https://*.sambray.org/*",
    "https://*.hydrus.sambray.org/*",
    "https://*.meteor.sambray.org/*",
    "https://*.indigo.sambray.org/*",
    "https://*.blue.sambray.org/*",
    "https://*.mimin.sambray.org/*",
    "https://*.nexus.sambray.org/*",
    "https://*.einsteinium.sambray.org/*",
    "https://*.lutetium.sambray.org/*",
    "https://*.fermium.sambray.org/*",
    "https://*.shadow.sambray.org/*",
    "https://*.tallium.sambray.org/*",
    "https://*.pixel.sambray.org/*",
    "https://*.silence.sambray.org/*",
    "https://*.voidboost.cc/*",
    "https://*.stream.voidboost.cc/*",
    "https://*.meteor.stream.voidboost.cc/*",
    "https://*.scorpius.stream.voidboost.cc/*",
    "https://*.lutetium.stream.voidboost.cc/*",
    "https://*.nexus.stream.voidboost.cc/*",
    "https://*.einsteinium.stream.voidboost.cc/*",
    "https://*.indigo.stream.voidboost.cc/*",
    "https://*.hydrus.stream.voidboost.cc/*",
    "https://*.fermium.stream.voidboost.cc/*",
    "https://*.shadow.stream.voidboost.cc/*",
    "https://*.blue.stream.voidboost.cc/*",
    "https://*.pixel.stream.voidboost.cc/*",
    "https://*.tallium.stream.voidboost.cc/*",
    "https://*.silence.stream.voidboost.cc/*",
    "https://*.consul.stream.voidboost.cc/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.hdrezka.ac/*",
        "https://*.rezka.ag/*",
        "https://*.rezka.cc/*",
        "https://*.hdrezka.ag/*",
        "https://*.rezka-ua.tv/*",
        "https://*.rezka-ua.net/*",
        "https://*.omnirezka.tv/*",
        "https://*.sambray.org/*",
        "https://*.hydrus.sambray.org/*",
        "https://*.meteor.sambray.org/*",
        "https://*.indigo.sambray.org/*",
        "https://*.blue.sambray.org/*",
        "https://*.mimin.sambray.org/*",
        "https://*.nexus.sambray.org/*",
        "https://*.einsteinium.sambray.org/*",
        "https://*.lutetium.sambray.org/*",
        "https://*.fermium.sambray.org/*",
        "https://*.shadow.sambray.org/*",
        "https://*.tallium.sambray.org/*",
        "https://*.pixel.sambray.org/*",
        "https://*.silence.sambray.org/*",
        "https://*.voidboost.cc/*",
        "https://*.stream.voidboost.cc/*",
        "https://*.meteor.stream.voidboost.cc/*",
        "https://*.scorpius.stream.voidboost.cc/*",
        "https://*.lutetium.stream.voidboost.cc/*",
        "https://*.nexus.stream.voidboost.cc/*",
        "https://*.einsteinium.stream.voidboost.cc/*",
        "https://*.indigo.stream.voidboost.cc/*",
        "https://*.hydrus.stream.voidboost.cc/*",
        "https://*.fermium.stream.voidboost.cc/*",
        "https://*.shadow.stream.voidboost.cc/*",
        "https://*.blue.stream.voidboost.cc/*",
        "https://*.pixel.stream.voidboost.cc/*",
        "https://*.tallium.stream.voidboost.cc/*",
        "https://*.silence.stream.voidboost.cc/*",
        "https://*.consul.stream.voidboost.cc/*"
      ],
      "js": ["bgAdsBlocker.js", "vidAdsBlocker.js", "url.js", "popup.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}